{
  "version": 2,
  "builds": [
    {
      "src": "api/telegram-webhook.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/telegram-webhook.js"
    }
  ],
  "npmInstallCommand": "npm i --legacy-peer-deps --force",
  "buildCommand": "npm run build || echo 'No build needed'"
}
